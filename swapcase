def swap_case(s):
    newStr = ""
    for i in s:
        if i.islower() == True:
            newStr = newStr + i.upper()
        elif i.isupper() == True:
            newStr = newStr + i.lower()
        else:
            newStr = newStr + i
    return newStr

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)